<% @band.users.each do |user| %>
  <ul class='list-inline'>
    <li class="list-inline-item"><%= cl_image_tag user.avatar.key, class:"avatar-large" %></li>
    <li class="list-inline-item"><%= user.first_name %></li>
    <li class="list-inline-item background-instrument-user"><%= user.instrument %></li>
  </ul>
<% end %>
